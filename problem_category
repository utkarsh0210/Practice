#include <bits/stdc++.h>

using namespace std;

int main() {
	ios::sync_with_stdio(false);
	cin.tie(0);
	int tt;
	cin >> tt;
	while (tt--) {
		int x;
		cin >> x;
		if (x < 100) {
			cout << "Easy";
		} else if (x < 200) {
			cout << "Medium";
		} else {
			cout << "Hard";
		}
		cout << '\n';
	}
	return 0;
}
